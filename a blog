//  计算一行整数中所有奇数的乘积，注意格式
#include<stdio.h>
int main()
{
	int m, n, sum;
	while( scanf("%d",&n) != EOF ){
		sum = 1;
		while(n--){
			scanf("%d",&m);
			if( m%2 == 1 )
				sum*=m;
		}
		printf("%d\n",sum);
	}
	return 0;
}
