//  计算一行整数中所有奇数的乘积，注意格式
#include<stdio.h>
int main()
{
	int m, n, sum;
	while( scanf("%d",&n) != EOF ){
		sum = 1;
		while(n--){
			scanf("%d",&m);
			if( m%2 == 1 )
				sum*=m;
		}
		printf("%d\n",sum);
	}
	return 0;
}
=========================================
//  给定一段连续的整数，求出他们中所有
偶数的平方和以及所有奇数的立方和。踩了个坑，
计算之前要记得判断两个数的大小，如果不判断
就直接计算可能会出错。
#include<stdio.h>
int main()
{
	int m,n,sum1,sum2,t;
	while( scanf("%d %d",&m,&n) != EOF ){
		sum1 = sum2 = 0;
		if(m > n){
			t = m;
			m = n;
			n = t;
		}
		while(m <= n){
			if(m%2 == 0)
				sum1 += m*m;
			else
				sum2 += m*m*m;
			m++;
		}
		printf("%d %d\n",sum1,sum2);
	}
	
	return 0;
}
=============================================3.15
